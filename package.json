{
  "name": "cococasing-storefront",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "scully": "npx scully --scanRoutes --noPrompt",
    "scully:serve": "npx scully serve",
    "config": "ts-node src/environments/set-env.ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "gql:fetch": "ts-node codegen/fetch-shopify-schema.ts",
    "gql:generate": "graphql-codegen --config codegen/codegen.ts",
    "gql:watch": "graphql-codegen --config codegen/codegen.ts --watch",
    "blog:serve": "concurrently \"npm run blog:watch\" \"npm run scully:serve\"",
    "blog:watch": "chokidar \"blog/**/*\" -c \"npm run scully\" --initial"
  },
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@apollo/client": "^3.0.0",
    "@popperjs/core": "^2.11.8",
    "@scullyio/init": "^2.1.41",
    "@scullyio/ng-lib": "^2.1.41",
    "@scullyio/scully": "^2.1.41",
    "@scullyio/scully-plugin-puppeteer": "^2.1.41",
    "apollo-angular": "^5.0.2",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "graphql": "^16",
    "hammerjs": "^2.0.8",
    "ngx-image-zoom": "^2.1.0",
    "ngx-toastr": "^16.2.0",
    "ngx-turnstile": "^0.0.7",
    "rxjs": "~7.5.0",
    "shopify-buy": "^2.20.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@parcel/watcher": "^2.3.0",
    "@scullyio/scully-plugin-flash-prevention": "^2.1.41",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/shopify-buy": "^2.17.2",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "autoprefixer": "^10.4.15",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.33.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.29",
    "scully-plugin-minify-html": "^6.0.0",
    "scully-plugin-time-to-read": "^0.0.5",
    "tailwindcss": "^3.3.3",
    "typescript": "~4.9.5"
  }
}
